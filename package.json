{
	"name": "workout-optimizer",
	"version": "1.0.0",
	"description": "Exercise Tracker App",
	"homepage": "https://github.com/Sebsterio/workout-optimizer#readme",
	"main": "server.js",
	"scripts": {
		"client": "npm start --prefix client",
		"client-test": "npm test --prefix client",
		"client-install": "npm install --prefix client",
		"start": "node server.js",
		"server": "nodemon server.js",
		"dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"concurrently": "^3.6.0",
		"config": "^3.0.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.16.3",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.9.19"
	},
	"devDependencies": {
		"morgan": "^1.9.1",
		"nodemon": "^1.17.5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Sebsterio/workout-optimizer.git"
	},
	"author": "Sebastian Rosloniec",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Sebsterio/workout-optimizer/issues"
	}
}
