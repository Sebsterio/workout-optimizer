{
	"name": "workout-optimizer",
	"description": "Exercise Tracker App",
	"version": "1.0.0",
	"author": "Sebastian Rosloniec",
	"license": "MIT",
	"main": "server.js",
	"scripts": {
		"client": "npm start --prefix client",
		"client-test": "npm test --prefix client",
		"client-install": "npm install --prefix client",
		"client-build": "npm run build --prefix client",
		"start": "node server.js",
		"server": "nodemon server.js",
		"dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
		"deploy": "git push origin && git push heroku master",
		"heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"concurrently": "^3.6.0",
		"config": "^3.0.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.16.3",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^5.9.19",
		"nvm": "^0.0.4"
	},
	"devDependencies": {
		"morgan": "^1.10.0",
		"nodemon": "^3.0.3"
	},
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=6.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Sebsterio/workout-optimizer.git"
	},
	"bugs": {
		"url": "https://github.com/Sebsterio/workout-optimizer/issues"
	}
}
